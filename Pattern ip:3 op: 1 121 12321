#include<iostream>
using namespace std;
int main()
{
    int n,k;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        k=i/1;
        if(k<=n)
        {
            for(int m=1;m<=k;m++)
            {
                cout<<m;
            }
            for(int m=k-1;m>=1;m--)
            {
                cout<<m;
            }
        }
        cout<<endl;
    }
    return 0;
}
